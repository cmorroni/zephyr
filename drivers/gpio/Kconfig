menuconfig GPIO
	bool "GPIO Drivers"
	select HAS_DTS_GPIO
	help
	  Include GPIO drivers in system config

if GPIO

module = GPIO
module-str = gpio
source "subsys/logging/Kconfig.template.log_config"

config GPIO_SHELL
	bool "Enable GPIO Shell"
	help
	  Enable GPIO Shell for testing.

source "drivers/gpio/Kconfig.intel"

endif # GPIO
